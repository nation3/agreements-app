{
  "name": "nation3-court-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn workspace @nation3/ui-components watch:all & next dev",
    "build": "yarn build:dependencies && yarn build:app",
    "start:all": "concurrently \"next dev\" \"yarn workspace @nation3/ui-components watch:all\"",
    "build:app": "next build",
    "build:dependencies": "yarn workspace @nation3/ui-components build:all",
    "start": "next start",
    "lint:check": "yarn lint:check:format && yarn lint:check:eslint",
    "lint:check:eslint": "next lint",
    "lint:check:format": "prettier --check **/*.tsx",
    "lint:fix": "prettier --write **/*.tsx",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.11",
    "@linkedmd/markdown-it-abbr": "^1.0.5",
    "@linkedmd/markdown-it-directive": "^1.0.1",
    "@linkedmd/parser": "^0.4.2",
    "@nation3/ui-components": "^0.1.0",
    "@rainbow-me/rainbowkit": "^0.7.1",
    "@safe-global/safe-core-sdk": "^3.2.1",
    "@safe-global/safe-ethers-lib": "^1.7.0",
    "@safe-global/safe-service-client": "^1.5.0",
    "@uniswap/permit2-sdk": "^1.2.0",
    "crypto-js": "^4.1.1",
    "eslint": "^8.26.0",
    "ethers": "^5.7.1",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "i18next": "^22.4.9",
    "ipfs-car": "^0.9.1",
    "keccak256": "^1.0.6",
    "markdown-it": "^13.0.1",
    "merkletreejs": "^0.3.0",
    "next": "latest",
    "next-i18next": "^13.0.3",
    "next-transpile-modules": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.4",
    "react-textarea-autosize": "^8.4.1",
    "sharp": "^0.31.3",
    "tippy.js": "^6.3.7",
    "wagmi": "^0.6.7",
    "web3.storage": "^4.4.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "cypress": "^10.11.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.2"
  }
}
