{
  "name": "@nation3/ui-components",
  "author": "Nation3 Solar Guild",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "styles.css"
  ],
  "types": "module",
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/styles.css"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "watch": "tsdx watch",
    "watch:css": "postcss src/styles.css -o dist/styles.css --watch",
    "watch:all": "concurrently \"yarn watch\" \"yarn watch:css\"",
    "build": "tsdx build",
    "build:css": "postcss src/styles.css -o dist/styles.css",
    "build:all": "concurrently \"yarn build\" \"yarn build:css\"",
    "test": "tsdx test --passWithNoTests",
    "lint:check": "yarn lint:check:format && yarn lint:check:eslint",
    "lint:check:eslint": "eslint src",
    "lint:check:format": "prettier --check **/*.tsx",
    "lint:fix": "prettier --write **/*.tsx",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.11",
    "@types/chokidar": "^2.1.3",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "ethers": "^5.7.2",
    "flowbite": "^1.5.3",
    "flowbite-react": "^0.1.11",
    "focus-trap-react": "^10.1.1",
    "framer-motion": "^7.5.1",
    "lottie-web": "^5.11.0",
    "react-blockies": "^1.4.1",
    "react-dropzone": "^14.2.2",
    "react-lottie": "^1.2.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@rollup/plugin-url": "^8.0.1",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/rollup": "^7.0.0",
    "@types/react-blockies": "^1.4.1",
    "@types/react-lottie": "^1.2.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-loader": "^8.2.5",
    "concurrently": "^8.0.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.7",
    "postcss": "^8.4.17",
    "postcss-cli": "^10.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.7.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svg": "^2.0.0",
    "sass": "^1.62.0",
    "storybook-dark-mode": "^2.0.4",
    "styled-components": "^5.3.9",
    "tailwindcss": "^3.1.8",
    "ts-loader": "^9.4.2",
    "tsdx": "^0.14.1",
    "typescript": "^5.0.3"
  }
}
